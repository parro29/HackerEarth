//All Tracks>Basic Programming> Implementation> Basics of Implementation> Problem
// ArraySum

// Write your code here
#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[10];
    long  sum = 0;
    for(int i = 0; i < n; i++)
    {
        cin>>arr[i];
    }
    for(int i = 0; i < n; i++)
    {
        sum += arr[i];
    }
    cout<<sum;
}
